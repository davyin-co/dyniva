        "drupal/user_default_page": "^2.0",
        "drupal/varbase_carousels": "^7.7",
        "drupal/varbase_core": "^8.28",
        "drupal/varbase_editor": "^7.13",
        "drupal/varbase_heroslider_media": "^7.16",
