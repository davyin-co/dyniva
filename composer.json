		"drupal/migrate_tools": "*",
		"drupal/select_or_other": "*",
		"drupal/shs": "*",
		"drupal/tour_ui": "*",
		"drupal/views_infinite_scroll": "*",
		"drupal/views_templates": "*",
		"drupal/viewsreference": "2.x",
		"drupal/multiselect": "*",
		"drupal/field_permissions": "*",
		"drupal/focal_point": "*",
		"drupal/module_filter": "*",
		"drupal/stage_file_proxy": "*",
