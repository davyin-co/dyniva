	"name": "davyin/dyniva",
	"description": "Daniva distribution, based on Drupal 8",
	"type": "drupal-profile",
	"license": "GPL-2.0+",
	"minimum-stability": "dev",
	"prefer-stable": true,
	"require": {
		"drupal-composer/drupal-scaffold": "^2.0.0",
